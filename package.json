{
  "private": true,
  "scripts": {
    "postinstall": "dotnet restore src",
    "start-server": "live-server deploy",
    "build-fable": "cd src && dotnet fable fable-splitter -- src/App.fsproj -o build --run",
    "start-fable": "yarn build-fable --watch",
    "start": "concurrently -k npm:start-*",
    "build": "concurrently -k npm:build-*",
    "deploy": "yarn build && gh-pages -d deploy"
  },
  "dependencies": {
    "concurrently": "^4.0.1",
    "fable-splitter": "^2.1.0-alpha-002",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "highlight.js": "^9.13.1",
    "live-server": "^1.2.0",
    "marked": "^0.5.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  }
}
